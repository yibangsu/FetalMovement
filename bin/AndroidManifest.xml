<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bang.fetalmovement"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.SEND_RESPOND_VIA_MESSAGE"/>
    <uses-permission android:name="android.permission.LOCATION_HARDWARE"/>
    <uses-permission android:name="android.permission.SCORE_NETWORKS"/>
    <uses-permission android:name="android.permission.RECEIVE_DATA_ACTIVITY_CHANGE"/>
    <!--  uses-permission android:name="android.permission.MODIFY_PHONE_STATE"/-->
    <uses-permission android:name="android.permission.OEM_UNLOCK_STATE"/>
    <!-- uses-permission android:name="android.permission.MANAGE_USB"/-->
    <!--  uses-permission android:name="android.permission.ACCOUNT_MANAGER"/-->
    <!--  uses-permission android:name="android.permission.DUMP"/-->
    
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/girl_kid"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.bang.fetalmovement.FetalMovement"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bang.fetalmovement.FetalMovementHelp"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bang.fetalmovement.FetalMovementHistory"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
